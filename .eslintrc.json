{
    "env": {
        "node": true
    },
    "extends": [
        "prettier-standard/lib/base",
        "eslint:recommended",
        "plugin:node/recommended",
        "prettier"
    ],
    "plugins": [
        "import",
        "node"
    ],
    "rules": {
        "prettier/prettier": "error",
        "node/no-unsupported-features": "off",
        "node/no-unpublished-require": 0,
        "no-console": "off",
        "no-new": "off"
    }
}